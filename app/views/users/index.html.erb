<div class="container">
  <div class="row">
    <h3>Users(<span class="user_count"></span>)</h3>
    <div class="col-lg-12 col-md-12 col-sm-12">
      <div id="users">
        <% @users.each do |user| %>
          <div class="col-md-3 col-sm-6 full-border margin-10 offline" id="user_<%= user.id %>">
            <h3>
              <%= user.name %>
            </h3>
            <h5>
              <%= user.email %>
            </h5>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>

<script>
  $(function(){
    var user_id = "<%= current_user.id %>";
    var user_socket = new UserSocket(user_id);
  });
</script>
